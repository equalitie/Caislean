server {
	listen	80 default_server;
	listen	[::]:80	default_server ipv6only=on;
	server_name _;

	expires -1;

	if ($request_method !~ ^(GET|HEAD|POST)$ )
	{
		return 405;
	}

	root /var/www/default;
}

server {
	listen   443 ssl default;
	listen	[::]:443 ssl default ipv6only=on;
	server_name _;

	expires -1;

	if ($request_method !~ ^(GET|HEAD|POST)$ )
	{
		return 405;
	}

	root /var/www/default;

}
